{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/brave-fx/browser",
  "framework": null,
  "routes": [
    {
      "src": "/(.*\\.(js|css|json|ico|png|jpg|jpeg|svg|gif|woff|woff2|ttf|eot|map))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
